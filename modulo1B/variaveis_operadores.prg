/*
	Operadores: +, -, *, /
*/

procedure main 
local nNumero1, nNumero2

	input "Digite o primeiro numero: "  to nNumero1
	input "Digite o segundo numero: "  to nNumero2

	? "Soma: " , nNumero1 + nNumero2
	? "Subtracao: " , nNumero1 - nNumero2
	? "Multuplicacao: " , nNumero1 * nNumero2
	? "Divisao: " , nNumero1 / nNumero2

return 